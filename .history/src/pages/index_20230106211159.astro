  },
  {
    name: "Linkedin",
    url: "https://www.linkedin.com/in/josedv1",
    icon: "linkedin.svg",
  },
];
const brand = { name: "JoseDv1 Web", logo: "images/logo.jpg" };
const links = [
  {
    name: "Home",
    url: "/",
  },
  {
    name: "About Me",
    url: "#AboutMe",
